{
  "name": "cps-store-backend",
  "version": "1.0.0",
  "description": "CS Store Backend - E-commerce API",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "dev": "npm run build && node dist/index.js",
    "audit:cluster-route": "ts-node src/scripts/clusterRouteAudit.ts",
    "import-pincodes": "ts-node src/scripts/importPincodes.ts",
    "seed-pincodes": "ts-node scripts/seedPincodes.ts",
    "seed-products": "ts-node src/scripts/seedProducts.ts",
    "seed-all": "npm run seed-pincodes && npm run seed-products",
    "migrate:addresses": "ts-node src/scripts/regeocode_missing_addresses.ts",
    "migrate:images": "ts-node scripts/migrate-images.ts",
    "fix-images": "ts-node scripts/fix-images.ts",
    "rebuild-images": "ts-node scripts/rebuild-images.ts",
    "delete:broken": "ts-node scripts/delete-broken-products.ts",
    "check:images": "ts-node scripts/check-images.ts",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false --reporters=default --reporters=jest-junit",
    "test:seed": "ts-node tests/helpers/seed.ts",
    "test:reset": "ts-node tests/helpers/reset.ts",
    "test:mock": "ts-node tests/helpers/enableMocks.ts",
    "test:integration": "jest tests/integration --runInBand",
    "test:unit": "jest tests/unit --runInBand",
    "test:all": "npm run test:unit && npm run test:integration",
    "lint": "eslint src --ext .ts,.tsx --fix"
  },
  "keywords": [
    "ecommerce",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.4.2",
    "@mapbox/polyline": "^1.2.1",
    "@sentry/node": "^7.120.4",
    "@sentry/types": "^10.18.0",
    "@types/node-cache": "^4.1.3",
    "algoliasearch": "^5.45.0",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "bullmq": "^5.65.0",
    "cloudinary": "^1.41.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "googleapis": "^161.0.0",
    "helmet": "^7.2.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "mongodb": "^6.5.0",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "nodemailer": "^7.0.9",
    "otp-generator": "^4.0.1",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "razorpay": "^2.9.6",
    "redis": "^4.6.10",
    "resend": "^6.1.2",
    "sharp": "^0.34.5",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "twilio": "^5.10.3",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/algoliasearch": "^3.34.11",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mapbox__polyline": "^1.0.5",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.10.4",
    "@types/nodemailer": "^7.0.4",
    "@types/otp-generator": "^4.0.2",
    "@types/passport": "^1.0.17",
    "@types/passport-facebook": "^3.0.3",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^3.0.13",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "eslint": "^9.37.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.2.3",
    "nodemon": "^3.0.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  }
}
